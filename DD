int main()
{
    int arr[3][3],i,j,s;
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            printf("Enter number ");
            scanf("%d",&arr[i][j]);
        }
    }
    for(i=0;i<3;i++)
    {
    s = 0;
    for(j=0;j<3;j++)
    s = s+arr[i][j];
    printf("%d\n",s);
    }
    return 0;
}
